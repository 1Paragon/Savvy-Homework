function findVowel(txt)
{
  var vowel = 'aeiouAEIOU';
  var vcount = 0;
  
  for(var x = 0; x < txt.length ; x++)
  {
    if (vowel.indexOf(txt[x]) !== -1)
    {
      vcount += 1;
    }
    }
  return vcount;
}
console.log(findVowel("hello"));
console.log(findVowel("why"))

