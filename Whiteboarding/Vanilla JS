//Write a function that determines if a string in a palindrome.
//A palindrome is a string that is the same forward and backwards.
//Examples:
//isPalindrome('racecar') // --> true
//isPalindrome('table') // --> false

function ifPalindrome(str) {
  let palindrome = str.split(“”);
  console.log(palindrome);
  palindrome = palindrome.reverse();
  console.log(palindrome);
  palindrome = palindrome.join(“”);
  return str === palindrome;
}
ifPalindrome(“racecar”);
function ifPalindrome(str) {
  let palindrome = str
    .split(“”)
    .reverse()
    .join(“”);
  return str === palindrome;
}
ifPalindrome(“table”);